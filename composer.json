{
    "name": "nfq-rho/banner-bundle",
    "description": "NFQ Banner bundle",
    "keywords": ["NFQ", "banner"],
    "type": "symfony-bundle",
    "authors": [
        {
            "name":     "NFQ",
            "homepage": "http://www.nfq.com/"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:nfq-rho/admin-bundle.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "~2.6",
        "symfony/monolog-bundle": "~2.6",
        "doctrine/orm": "~2.5",
        "doctrine/doctrine-bundle": "~1.5",
        "sensio/framework-extra-bundle": "~3.0",

        "gedmo/doctrine-extensions": "2.4.37",
        "gregwar/image-bundle": "dev-master",
        "stfalcon/tinymce-bundle": "~1.1|~2.1",

        "nfq-rho/admin-bundle": "~0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7"
    },
    "autoload": {
        "psr-4": { "Nfq\\BannerBundle\\": "" }
    },
    "branch-alias" : {
        "dev-master" : "0.x-dev"
    }
}
